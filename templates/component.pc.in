prefix=@CMAKE_INSTALL_PREFIX@
libdir=${prefix}/@PKG_CONFIG_LIB_INSTALL_DIR@
libname=@PKG_CONFIG_LIB_NAME@
includedir=${prefix}/@PKG_CONFIG_INCLUDE_INSTALL_DIR@

Name: @PKG_CONFIG_COMPONENT_NAME@
Description: @PKG_CONFIG_COMPONENT_DESCRIPTION@
Version: @PKG_CONFIG_COMPONENT_VERSION@
Requires: @PKG_CONFIG_COMPONENT_REQUIRES@
Libs: -L${libdir} -l${libname} @PKG_CONFIG_LINK_FLAGS@
Cflags: -I${includedir} @PKG_CONFIG_CFLAGS@
